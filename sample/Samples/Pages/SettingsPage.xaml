<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Samples.SettingsPage"
             Title="Settings/Themes">
    <ContentPage.Content>
        <TableView>
            <TableSection>
                <TextCell Text="Load Standard Theme" Command="{Binding StandardSettings}" />
                <TextCell Text="Load Funny Theme" Command="{Binding LoadAbnormalSettings}" />
            </TableSection>
        </TableView>
    </ContentPage.Content>
</ContentPage>